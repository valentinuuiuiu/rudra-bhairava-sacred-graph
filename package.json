{
  "name": "piata-ro",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "python manage.py runserver",
    "dev": "python manage.py runserver --settings=piata_ro.settings",
    "build": "python manage.py collectstatic --noinput",
    "test": "python manage.py test",
    "migrate": "python manage.py migrate",
    "makemigrations": "python manage.py makemigrations",
    "createsuperuser": "python manage.py createsuperuser",
    "collectstatic": "python manage.py collectstatic --noinput",
    "agents": "cd awesome-mcp-servers && ./start-agents.sh",
    "agents:all": "cd awesome-mcp-servers && ./start-agents.sh all",
    "agents:help": "cd awesome-mcp-servers && ./start-agents.sh help"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14"
  }
}
